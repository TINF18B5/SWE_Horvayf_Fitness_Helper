<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.TrainingPlanFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view_training_plan"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/muscleplan_item" />

    <io.github.yavski.fabspeeddial.FabSpeedDial
        android:id="@+id/button_speed_dial_training_plan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:fabGravity="bottom_end"
        app:fabDrawable="@drawable/ic_more"
        app:fabMenu="@menu/fab_speed_dial_menu"/>

</androidx.constraintlayout.widget.ConstraintLayout>